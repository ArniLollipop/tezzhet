{
	"extends": ["@nuxtjs/eslint-config-typescript", "plugin:prettier/recommended"],
	"rules": {
		"prettier/prettier": [
			"warn",
			{
				"endOfLine": "auto"
			}
		],
		"quotes": ["error", "double"],
		"semi": ["error", "always"],
		"standard/no-callback-literal": 0,
		"vue/multi-word-component-names": "off"
	}
}
